            "@babel/plugin-proposal-class-properties"
        ]
    },
    "dependencies": {
        "@mdx-js/mdx": "^1.5.0",
        "@mdx-js/react": "^1.5.0",
        "compass-mixins": "^0.12.10",
        "gatsby": "^2.20.4",
        "gatsby-plugin-google-analytics": "^2.1.19",
        "gatsby-plugin-manifest": "^2.3.2",
        "gatsby-plugin-mdx": "^1.0.46",
        "gatsby-plugin-offline": "^3.0.11",
        "gatsby-plugin-react-helmet": "^3.1.10",
        "gatsby-plugin-sass": "^2.1.17",
        "gatsby-plugin-sharp": "^2.5.3",
        "gatsby-source-filesystem": "^2.1.28",
        "gatsby-transformer-remark": "^2.6.26",
        "gatsby-transformer-sharp": "^2.4.2",
        "mathsass": "^0.10.1",
        "prop-types": "^15.7.2",
